{
    "name": "zislogic/ebay-connector",
    "description": "eBay OAuth 2.0 and infrastructure package for Laravel 12",
    "keywords": ["laravel", "ebay", "oauth", "api", "connector"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/encryption": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Zislogic\\Ebay\\Connector\\": "src/",
            "Zislogic\\Ebay\\Connector\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zislogic\\Ebay\\Connector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zislogic\\Ebay\\Connector\\EbayConnectorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=512M"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
